# Test environment configuration for Playwright tests
# Copy this file to .env.test and adjust URLs as needed

# Service management mode
# Let Playwright manage services (default)
# Set to false if you want to manage services manually
PLAYWRIGHT_MANAGED_SERVICES=true

# Frontend URL (where the React app is running in testing mode)
FRONTEND_URL=http://localhost:3100

# Backend URL (where the API server is running in testing mode)
BACKEND_URL=http://localhost:5100