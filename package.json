{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "pnpm generate:api:build && pnpm generate:routes && tsc -b && vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "preview": "vite preview",
    "generate:routes": "tsr generate",
    "generate:api": "node scripts/generate-api.js --fetch",
    "generate:api:build": "node scripts/generate-api.js --cache-only"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@tanstack/react-router": "^1.131.27",
    "@tanstack/router-cli": "^1.131.27",
    "@tanstack/router-devtools": "^1.131.27",
    "@tanstack/react-query": "^5.0.0",
    "openapi-fetch": "^0.12.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "openapi-typescript": "^7.0.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
